модул маин

функция маин() {
	# коментар
	нека п1 = 100
	принтлн(„п1= $p1“)
	нека п2 = примерно_число()
	принтлн(„п2= $p2“)
	нека п3 = 12.145
	принтлн(„п3= $p3“)
	нека п4 = примерно_ф64()
	принтлн(„п4= $p4“)
	нека п5 = 'низ'
	принтлн(„п5= $p5“)
	нека п6 = примерен_низ()
	принтлн(„п6= $p6“)
	нека п7 = вярно
	принтлн(„п7= $p7“)
	нека п8 = примерно_бул()
	принтлн(„п8= $p8“)
	нека п9 = функция(ч число) число {
		върни ч + 1
	}
	принтлн(„п9= $p9“)
	нека п10 = примерна_функция()
	принтлн(„п10= $p10“)
	пром п11 = 10
	п11 = 11
	принтлн(„п11= $p11“)
	нека п12 = [1, 2, 3]
	принтлн(„п12= $p12“)
}

функция примерно_число() число {
	върни 1
}

функция примерно_ф64() ф64 {
	върни 1.2
}

функция примерен_низ() низ {
	върни „низ низ“
}

функция примерно_бул() бул {
	върни невярно
}

функция примерна_функция() функция(ч число) число {
	върни функция(ч число) число {
		върни ч + 10
	}
}
