модул маин

функция маин() {
	# коментар
	нека п1 = 100
	принтнилн(„п1= $p1“)
	нека п2 = примерно_ч32()
	принтнилн(„п2= $p2“)
	нека п3 = 12.145
	принтнилн(„п3= $p3“)
	нека п4 = примерно_ф64()
	принтнилн(„п4= $p4“)
	нека п5 = 'низ'
	принтнилн(„п5= $p5“)
	нека п6 = примерен_низ()
	принтнилн(„п6= $p6“)
	нека п7 = вярно
	принтнилн(„п7= $p7“)
	нека п8 = примерно_бул()
	принтнилн(„п8= $p8“)
	нека п9 = функция(ч ч32) ч32 {
		върни ч + 1
	}
	принтнилн(„п9= $p9“)
	нека п10 = примерна_функция()
	принтнилн(„п10= $p10“)
	пром п11 = 10
	п11 = 11
	принтнилн(„п11= $p11“)
	нека п12 = [1, 2, 3]
	принтнилн(„п12= $p12“)
}

функция примерно_ч32() ч32 {
	върни 1
}

функция примерно_ф64() ф64 {
	върни 1.2
}

функция примерен_низ() низ {
	върни „низ низ“
}

функция примерно_бул() бул {
	върни невярно
}

функция примерна_функция() функция(ч ч32) ч32 {
	върни функция(ч ч32) ч32 {
		върни ч + 10
	}
}
